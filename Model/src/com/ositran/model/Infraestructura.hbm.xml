<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.ositran.model">
    <class name="Infraestructura" table="T_INFRAESTRUCTURA">
        <id name="infId" type="java.lang.Integer" access="field">
            <column name="INF_ID" />
            <generator class="increment">
                <param name="sequence">EmpresaSupervisora_Id_Seq_Gen</param>
            </generator>
        </id>
            <property name="infFechaAlta" type="java.util.Date" column="INF_FECHA_ALTA"/>
            <property name="infFechaBaja" type="java.util.Date" column="INF_FECHA_BAJA"/> 
            <property name="infFechaCambio" type="java.util.Date" column="INF_FECHA_CAMBIO"/> 
            <property name="infNombre" type="string" column="INF_NOMBRE"/> 
            <property name="infTerminal" type="string" column="INF_TERMINAL"/> 
            <property name="infUsuarioAlta" type="string" column="INF_USUARIO_ALTA"/> 
            <property name="infUsuarioBaja" type="string" column="INF_USUARIO_BAJA"/> 
            <property name="infUsuarioCambio" type="string" column="INF_USUARIO_CAMBIO"/>   
            <property name="csiId" type="java.lang.Integer" column="CSI_ID"/>
            
        
       
    </class>
</hibernate-mapping>
