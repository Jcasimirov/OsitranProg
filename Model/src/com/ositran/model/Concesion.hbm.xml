<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.ositran.model">
    <class name="Concesion" table="T_CONCESION">
        <id name="conId" type="java.lang.Integer" access="field">
            <column name="CON_ID" />
            <generator class="increment">
                <param name="sequence">T_CONCESION_ID_SEQ_GEN</param>
            </generator>
        </id>
        
        <property name="conTipoinfra" type="string" column="CON_TIPOINFRA"/>
        <property name="conNombre" type="string" column="CON_NOMBRE"/>
        <property name="conNominfra" type="string" column="CON_NOMINFRA"/>
        
        
    </class>
</hibernate-mapping>