<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.ositran.model">
    <class name="Concesionario" table="T_CONCESIONARIO">
        <id name="cncId" type="java.lang.Integer" access="field">
            <column name="CNC_ID" />
            <generator class="increment">
                <param name="sequence">SEQ_CONCESIONARIO</param>
            </generator>
        </id>
        <property name="cncCorreo" type="string" column="CNC_CORREO"/>
        <property name="cncDescripcion" type="string" column="CNC_DESCRIPCION"/>
        <property name="cncDireccion" type="string" column="CNC_DIRECCION"/>
        <property name="cncNombre" type="string" column="CNC_NOMBRE"/>
        <property name="cncEstado" type="java.lang.Integer" column="CNC_ESTADO"/>
        <property name="cncNroDocumento" type="string" column="CNC_NRO_DOCUMENTO"/>
        <property name="cncRepresentanteLegal" type="string" column="CNC_REPRESENTANTE_LEGAL"/>
        <property name="cncTelefono" type="string" column="CNC_TELEFONO"/>
        <property name="cncTerminal" type="string" column="CNC_TERMINAL"/>
        <property name="cncFechaAlta" type="java.util.Date" column="CNC_FECHA_ALTA"/>
        <property name="cncFechaBaja" type="string" column="CNC_FECHA_BAJA"/>
        <property name="cncFechaCambio" type="java.util.Date" column="CNC_FECHA_CAMBIO"/>
        <property name="cncUsuarioAlta" type="string" column="CNC_USUARIO_ALTA"/>	
        <property name="cncUsuarioBaja" type="java.util.Date" column="CNC_USUARIO_BAJA"/>
        <property name="cncUsuarioCambio" type="string" column="CNC_USUARIO_CAMBIO"/>
	<property name="crgId" type="java.lang.Integer" column="CRG_ID"/>
        <property name="tdoId" type="java.lang.Integer" column="TDO_ID"/>
    </class>
</hibernate-mapping>