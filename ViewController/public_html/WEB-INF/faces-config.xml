<?xml version="1.0" encoding="windows-1252"?>
<faces-config version="2.1" xmlns="http://java.sun.com/xml/ns/javaee">
   <application>
	<resource-bundle>
		<base-name>com.ositran.bundle.titulos</base-name>
		<var>msg</var>			
	</resource-bundle>
	<resource-bundle>
		<base-name>com.ositran.bundle.botones</base-name>
		<var>btn</var>			
	</resource-bundle>
	<resource-bundle>
		<base-name>com.ositran.bundle.etiquetas</base-name>
		<var>etq</var>			
	</resource-bundle>
	<resource-bundle>
		<base-name>com.ositran.bundle.legend</base-name>
		<var>leg</var>			
	</resource-bundle>
		<resource-bundle>
		<base-name>com.ositran.bundle.campo</base-name>
		<var>cam</var>			
	</resource-bundle>
	<resource-bundle>
		<base-name>com.ositran.bundle.datatable</base-name>
		<var>tab</var>			
	</resource-bundle>
    </application>
    <application>
        <variable-resolver>
            org.springframework.web.jsf.SpringBeanVariableResolver
        </variable-resolver>
    </application>
    <application>
        <el-resolver>
            org.springframework.web.jsf.el.SpringBeanFacesELResolver
        </el-resolver>
    </application>
     <navigation-rule>
      <from-view-id>/ositran/seguridad/mantenimientoUsuario.jsf</from-view-id>
      <navigation-case>
       <from-outcome>usuarioUpd</from-outcome>
       <to-view-id>/ositran/seguridad/usuarioUpd.jsf</to-view-id>
      </navigation-case>
     </navigation-rule>
 <navigation-rule>
  <from-view-id>/ositran/logueo.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>success</from-outcome>
   <to-view-id>/ositran/seguridad/menu.xhtml</to-view-id>
  </navigation-case>
  <navigation-case>
   <from-outcome>error</from-outcome>
   <to-view-id>/error.jsf</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/ositran/seguridad/menu.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>crudTipInf</from-outcome>
   <to-view-id>/ositran/parametros/mantenimientoTipoInfraestructura.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/ositran/parametros/mantenimientoTipoInfraestructura.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>regTipInf</from-outcome>
   <to-view-id>/ositran/parametros/registrarTipoInfraestructura.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
</faces-config>