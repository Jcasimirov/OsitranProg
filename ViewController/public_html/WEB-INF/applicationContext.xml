    <bean id="inversionDescripcionVO" class="com.ositran.vo.bean.InversionDescripcionVO" />
    <bean id="inversionTipoDescripcion" class="com.ositran.model.InversionTipoDescripcion" />
    <bean id="rolVO" class="com.ositran.vo.bean.RolVO" />
    <bean id="rol" class="com.ositran.model.Rol" />
    <bean id="menVO" class="com.ositran.vo.bean.MenVO" />
    <bean id="men" class="com.ositran.model.Men" />
    <bean id="rolOpcionesVO" class="com.ositran.vo.bean.RolOpcionesVO" />
    <bean id="rolOpciones" class="com.ositran.model.RolOpciones" />
    <bean id="concesionVO" class="com.ositran.vo.bean.ConcesionVO" />
    <bean id="concesion" class="com.ositran.model.Concesion" />
    <bean id="infraestructuraTipo" class="com.ositran.model.InfraestructuraTipo" />
    <bean id="infraestructuraTipoVO" class="com.ositran.vo.bean.InfraestructuraTipoVO" />
    <bean id="usuario" class="com.ositran.model.Usuario" />
    <bean id="usuarioVO" class="com.ositran.vo.bean.UsuarioVO" />
    <bean id="infraestructuraVO" class="com.ositran.vo.bean.InfraestructuraVO" />
    <bean id="infraestructura" class="com.ositran.model.Infraestructura" />


    <!--Concesion e Infraestructura-->
    <bean id="infraestructuraDAOImpl" class="com.ositran.daoimpl.InfraestructuraDAOImpl"/>
    <bean id="infraestructuraServiceImpl" class="com.ositran.serviceimpl.InfraestructuraServiceImpl">
        <property name="infraestructuraDAOImpl" ref="infraestructuraDAOImpl"/>
    </bean>
    <bean id="concesionDAOImpl" class="com.ositran.daoimpl.ConcesionDAOImpl"/>
    <bean id="concesionServicesImpl" class="com.ositran.serviceimpl.ConcesionServiceImpl">
        <property name="concesionDAOImpl" ref="concesionDAOImpl"/>
    </bean>
    <bean id="mantenimientoConcesion" class="com.ositran.parametros.MantenimientoConcesion">
        <property name="concesionServicesImpl" ref="concesionServicesImpl"/>
        <property name="infraestructuraTipoServiceImpl" ref="infraestructuraTipoServiceImpl"/>
        <property name="infraestructuraServiceImpl" ref="infraestructuraServiceImpl"/>
        <property name="concesionVO" ref="concesionVO"/>
        <property name="infraestructuraVO" ref="infraestructuraVO"/>
    </bean>
    
    <!--Concesionario-->
    <bean id="concesionarioDAOImpl" class="com.ositran.daoimpl.ConcesionarioDAOImpl">
        <property name="concesionario" ref="concesionario"/>
    </bean>
    <bean id="concesionarioServiceImpl" class="com.ositran.serviceimpl.ConcesionarioServiceImpl">
        <property name="concesionarioDAOImpl" ref="concesionarioDAOImpl"/>
        <property name="concesionario" ref="concesionario"/>
        <property name="concesionarioVO" ref="concesionarioVO"/>
    </bean>
    <bean id="consesionario" class="com.ositran.parametros.Concesionario">
        <property name="tipoDocumentoServiceImp" ref="tipoDocumentoServiceImp"/>
        <property name="concesionarioServiceImpl" ref="concesionarioServiceImpl"/>
        <property name="cargoServiceImp" ref="cargoServiceImp"/>
        <property name="concesionarioVO" ref="concesionarioVO"/>
    </bean>
    <!--Igv-->
    <bean id="igvDAOImpl" class="com.ositran.daoimpl.IgvDAOImpl"/>
    <bean id="igvServiceImpl" class="com.ositran.serviceimpl.IgvServiceImpl">
        <property name="igvDAOImpl" ref="igvDAOImpl"/>
    </bean>
    <bean id="mantenimientoIgv" class="com.ositran.parametros.MantenimientoIgv">
        <property name="igvServiceImpl" ref="igvServiceImpl"/>
    </bean>
    <!--Empresa supervisora-->
    <bean id="cargoDAOImpl" class="com.ositran.daoimpl.CargoDAOImpl"/>
    <bean id="cargoServiceImp" class="com.ositran.serviceimpl.CargoServiceImpl">
        <property name="cargoDAOImpl" ref="cargoDAOImpl"/>
    </bean>
    <bean id="tipodocumentoDAOImpl" class="com.ositran.daoimpl.TipoDocumentoDAOImpl"/>
    <bean id="tipoDocumentoServiceImp" class="com.ositran.serviceimpl.TipoDocumentoServiceImpl">
        <property name="tipodocumentoDAOImpl" ref="tipodocumentoDAOImpl"/>
    </bean>
    <bean id="empresaSupervisoraDAOImpl" class="com.ositran.daoimpl.EmpresaSupervisoraDAOImpl"/>
    <bean id="empSupServiceImp" class="com.ositran.serviceimpl.EmpresaSupervisoraServiceImpl">
        <property name="empresaSupervisoraDAOImpl" ref="empresaSupervisoraDAOImpl"/>
    </bean>
    <bean id="mantenimientoEmpSup" class="com.ositran.parametros.MantenimientoEmpSup">
        <property name="empSupServiceImp" ref="empSupServiceImp"/>
        <property name="cargoServiceImp" ref="cargoServiceImp"/>
        <property name="tipoDocumentoServiceImp" ref="tipoDocumentoServiceImp"/>
    </bean>
    <bean id="empSupVO" class="com.ositran.vo.bean.EmpresaSupervisoraVO"></bean>
    <!--Inversion descripcion-->
    <bean id="inversionDescripcionDAOimpl" class="com.ositran.daoimpl.InversionDescripcionDAOImpl">
        <property name="inversionTipoDescripcion" ref="inversionTipoDescripcion"/>
    </bean>
    <bean id="inversionDescripcionServicesImpl" class="com.ositran.serviceimpl.InversionDescripcionServicesImpl">
        <property name="inversionDescripcionDAOimpl" ref="inversionDescripcionDAOimpl"/>
        <property name="inversionDescripcionVO" ref="inversionDescripcionVO"/>
        <property name="inversionTipoDescripcion" ref="inversionTipoDescripcion"/>
    </bean>
    <bean id="mantenimientoTipoInversionDescripcion" class="com.ositran.parametros.DescripcionTipoInversion">
        <property name="inversionDescripcionServicesImpl" ref="inversionDescripcionServicesImpl"/>
        <property name="tipoInversionServicesImpl" ref="tipoInversionServicesImpl"/>
        <property name="inversionDescripcionVO" ref="inversionDescripcionVO"/>
    </bean>
    <!--Tipo de inversiÃ³n-->
    <bean id="tipoInversionDAOImpl" class="com.ositran.daoimpl.TipoInversionDAOImpl">
        <property name="inversionTipo" ref="inversionTipo"/>
    </bean>
    <bean id="tipoInversionServicesImpl" class="com.ositran.serviceimpl.TipoInversionServiceImpl">
        <property name="tipoInversionDAOImpl" ref="tipoInversionDAOImpl"/>
        <property name="tipoInversionVO" ref="tipoInversionVO"/>
        <property name="inversionTipo" ref="inversionTipo"/>
    </bean>
    <bean id="mantenimientoTipoInversion" class="com.ositran.parametros.TipoInversion">
        <property name="tipoInversionServicesImpl" ref="tipoInversionServicesImpl"/>
    </bean>
    
    <!--Tipo de infraestructura-->
    <bean id="infraestructuraTipoDAOImpl" class="com.ositran.daoimpl.InfraestructuraTipoDAOImpl">
		<property name="infraestructuraTipo" ref="infraestructuraTipo"/>
    </bean>
	<bean id="infraestructuraTipoServiceImpl" class="com.ositran.serviceimpl.InfraestructuraTipoServiceImpl">
        <property name="infraestructuraTipoDAOImpl" ref="infraestructuraTipoDAOImpl"/>
        <property name="infraestructuraTipoVO" ref="infraestructuraTipoVO"/>
        <property name="infraestructuraTipo" ref="infraestructuraTipo"/>
    </bean>
	<bean id="mantenimientoInfraestructuraTipo" class="com.ositran.parametros.MantenimientoTipoInfraestructura">
        <property name="infraestructuraTipoVO" ref="infraestructuraTipoVO"/>
    </bean>
    
   
    <!--Modalidad de Concesion-->
    <bean id="modalidadVO" class="com.ositran.vo.bean.ModalidadConcesionVO"></bean>
    <bean id="modalidadConcesionDAOImpl" class="com.ositran.daoimpl.ModalidadConcesionDAOImpl"/>
    <bean id="modalidadServiceImp" class="com.ositran.serviceimpl.ModalidadConcesionServiceImpl">
        <property name="modalidadConcesionDAOImpl" ref="modalidadConcesionDAOImpl"/>
    </bean>
    <bean id="mantenimientoModalidadConcesion" class="com.ositran.parametros.MantenimientoModalidadConcesion">
        <property name="modalidadServiceImp" ref="modalidadServiceImp"/>
        
    </bean>
    <!--S E G U R I D A D-->
    <!--Usuario-->
    <bean id="usuarioDAOImpl" class="com.ositran.daoimpl.UsuarioDAOImpl">
		<property name="usuario" ref="usuario"/>
    </bean>
	<bean id="usuarioServiceImpl" class="com.ositran.serviceimpl.UsuarioServiceImpl">
        <property name="usuarioDAOImpl" ref="usuarioDAOImpl"/>
        
    </bean>
	<bean id="mantenimientoUsuario" class="com.ositran.seguridad.MantenimientoUsuario">
        <property name="usuarioVO" ref="usuarioVO"/>
    </bean>
    
    
    <!--Rol-->
    <bean id="rolDAOImpl" class="com.ositran.daoimpl.RolDAOImpl">
            <property name="rol" ref="rol"/>
    </bean>
    <bean id="rolServiceImpl" class="com.ositran.serviceimpl.RolServiceImpl">
        <property name="rolDAOImpl" ref="rolDAOImpl"/>
        <property name="rol" ref="rol"/>
        <property name="rolVO" ref="rolVO"/>
       

    </bean>
    <bean id="rolMB" class="com.ositran.seguridad.RolMB">
        <property name="rolServiceImpl" ref="rolServiceImpl"/>
        <property name="rolVO" ref="rolVO"/>
        <property name="menServiceImpl" ref="menServiceImpl"/>
        <property name="menVO" ref="menVO"/>
        <property name="rolOpcionesServiceImpl" ref="rolOpcionesServiceImpl"/>
    </bean>

<!--Menu-->
    <bean id="menDAOImpl" class="com.ositran.daoimpl.MenDAOImpl">
            <property name="men" ref="men"/>
    </bean>
    <bean id="menServiceImpl" class="com.ositran.serviceimpl.MenServiceImpl">
        <property name="menDAOImpl" ref="menDAOImpl"/>
        <property name="men" ref="men"/>
        <property name="menVO" ref="menVO"/>
    </bean>
	<bean id="opcionesSistema" class="com.ositran.seguridad.OpcionesSistemaMB">
        <property name="menServiceImpl" ref="menServiceImpl"/>
        <property name="menVO" ref="menVO"/>
    </bean>
  

<!--Rol Opciones-->
    <bean id="rolOpcionesDAOImpl" class="com.ositran.daoimpl.RolOpcionesDAOImpl">
            <property name="rolOpciones" ref="rolOpciones"/>
    </bean>
    <bean id="rolOpcionesServiceImpl" class="com.ositran.serviceimpl.RolOpcionesServiceImpl">
        <property name="rolOpcionesDAOImpl" ref="rolOpcionesDAOImpl"/>
        <property name="rolOpciones" ref="rolOpciones"/>
        <property name="rolOpcionesVO" ref="rolOpcionesVO"/>
    </bean>
    
</beans>