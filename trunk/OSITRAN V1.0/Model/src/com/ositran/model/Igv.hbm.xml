<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.ositran.model">
    <class name="Igv" table="T_IGV">
        <id name="igvId" type="java.lang.Integer" access="field">
            <column name="IGV_ID" />
            <generator class="increment">
                <param name="sequence">SEQ_IGV</param>
            </generator>
        </id>
        <property name="igvEstado" type="java.lang.Integer" column="IGV_ESTADO"/>
        <property name="igvFechaAlta" type="java.util.Date" column="IGV_FECHA_ALTA"/>
		<property name="igvFechaBaja" type="java.util.Date" column="IGV_FECHA_BAJA"/>
		<property name="igvFechaCambio" type="java.util.Date" column="IGV_FECHA_CAMBIO"/>
		<property name="igvFechacaducidad" type="java.util.Date" column="IGV_FECHACADUCIDAD"/>
		<property name="igvFechavigencia" type="java.util.Date" column="IGV_FECHAVIGENCIA"/>
		<property name="igvPorcentaje" type="java.lang.Double" column="IGV_PORCENTAJE"/>		
		<property name="igvTerminal" type="string" column="IGV_TERMINAL"/>
		<property name="igvUsuarioAlta" type="string" column="IGV_USUARIO_ALTA"/>
		<property name="igvUsuarioBaja" type="string" column="IGV_USUARIO_BAJA"/>
		<property name="igvUsuarioCambio" type="string" column="IGV_USUARIO_CAMBIO"/>	
       
    </class>
</hibernate-mapping>