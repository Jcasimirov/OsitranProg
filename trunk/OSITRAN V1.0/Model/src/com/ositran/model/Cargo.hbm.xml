<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.ositran.model">
    <class name="Cargo" table="T_CARGO">
        <id name="crgId" type="java.lang.Integer" access="field">
            <column name="CRG_ID" />
                <generator class="native">
                </generator>
        </id>
        <property name="crgDescripcion" type="string" column="CRG_DESCRIPCION"/>
        <property name="crgEstado" type="java.lang.Integer" column="CRG_ESTADO"/>
        <property name="crgFechaAlta" type="java.util.Date" column="CRG_FECHA_ALTA"/>
        <property name="crgFechaBaja" type="java.util.Date" column="CRG_FECHA_BAJA"/>
        <property name="crgFechaCambio" type="java.util.Date" column="CRG_FECHA_CAMBIO"/>
        <property name="crgNombre" type="string" column="CRG_NOMBRE"/>
        <property name="crgTerminal" type="string" column="CRG_TERMINAL"/>
        <property name="crgUsuarioAlta" type="string" column="CRG_USUARIO_ALTA"/>
        <property name="crgUsuarioBaja" type="string" column="CRG_USUARIO_BAJA"/>
        <property name="crgUsuarioCambio" type="string" column="CRG_USUARIO_CAMBIO"/>
    </class>
</hibernate-mapping>