<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.ositran.model">
    <class name="Rol" table="T_ROL">
        <id name="rolId" type="java.math.BigDecimal" access="field">
            <column name="ROL_ID" />
                <generator class="native">
                </generator>
        </id>        
        <property name="rolDescripcion" type="java.lang.Integer" column="ROL_DESCRIPCION"/>
        <property name="rolEstado" type="java.math.BigDecimal" column="ROL_ESTADO"/>
        <property name="rolFechaAlta" type="java.util.Date" column="ROL_FECHA_ALTA"/>
        <property name="rolFechaBaja" type="java.util.Date" column="ROL_FECHA_BAJA"/>
        <property name="rolFechaCambio" type="java.util.Date" column="ROL_FECHA_CAMBIO"/>
        <property name="rolNombre" type="string" column="ROL_NOMBRE"/>
        <property name="rolTerminal" type="string" column="ROL_TERMINAL"/>
        <property name="rolUsuarioAlta" type="string" column="ROL_USUARIO_ALTA"/>
        <property name="rolUsuarioBaja" type="string" column="ROL_USUARIO_BAJA"/>
        <property name="rolUsuarioCambio" type="string" column="ROL_USUARIO_CAMBIO"/>
    </class>
</hibernate-mapping>