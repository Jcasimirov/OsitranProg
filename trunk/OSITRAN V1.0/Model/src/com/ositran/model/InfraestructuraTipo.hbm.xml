<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.ositran.model">
    <class name="InfraestructuraTipo" table="T_INFRAESTRUCTURA_TIPO">
        <id name="tinId" type="java.lang.Integer" access="field">
            <column name="TIN_ID" />
            <generator class="increment">
                <param name="sequence">INFRAESTRUCTURATIPO_ID_SEQ_GEN</param>
            </generator>
        </id>
        <property name="tinDescripcion" type="string" column="TIN_DESCRIPCION"/>
        <property name="tinEstado" type="java.lang.Integer" column="TIN_ESTADO"/>        
        <property name="tinFechaAlta" type="java.util.Date" column="TIN_FECHA_ALTA"/>
        <property name="tinFechaBaja" type="java.util.Date" column="TIN_FECHA_BAJA"/>
        <property name="tinFechaCambio" type="java.util.Date" column="TIN_FECHA_CAMBIO"/>
        <property name="tinNombre" type="string" column="TIN_NOMBRE"/>
        <property name="tinTerminal" type="string" column="TIN_TERMINAL"/>
        <property name="tinUsuarioAlta" type="string" column="TIN_USUARIO_ALTA"/>
        <property name="tinUsuarioBaja" type="string" column="TIN_USUARIO_BAJA"/>
        <property name="tinUsuarioCambio" type="string" column="TIN_USUARIO_CAMBIO"/>
        
    </class>
</hibernate-mapping>